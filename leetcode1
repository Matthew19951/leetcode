def TwoSum(nums, target):
  dic = dict() 
  for index, value in enumerate(nums):
    #[(0,2), (1,7), (2,11), (3,15)], (index,value)
    sub = target - value #[7, 2, -2, -6]
    if sub in dic:
      return [dic[sub], index] 
    else: dic[value] = index 

nums = [2, 7, 11, 15]
target = 9
result = TwoSum(nums, target)
result
